						<% if(user){ %>
							<a href="" type="submit" id="<%= data._id %>"
								onclick="addWishlist('<%= data._id %>')">
								<i class="bi bi-heart mt-3 ms-3 fs-4" style="color: black;"></i>
							</a>
							<% } %>
					</div>
					<a href="/single-product?id=<%=data._id%>"><img src="/images/<%=data.images[0]%>"
							style="width: 8rem; height: 12rem;"></a>
					<figcaption>
						<%= data.productName %> <br> â‚¹<%= data.salePrice %>
					</figcaption>
